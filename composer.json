{
    "name": "alyka/smartsms-notification-channel",
    "description": "Smart SMS Solutions Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "smart sms solutions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alika Matthew",
            "email": "milimat@yahoo.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "composer/package-versions-deprecated": "^1.8",
        "illuminate/notifications": "~5.8.0|^6.0|^7.0|^8.0",
		"illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0",
        "guzzlehttp/guzzle": "^6.2 || ^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Alyka\\SmartSMS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminate\\Tests\\Notifications\\": "tests/"
        }
    },
	"scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alyka\\SmartSMS\\SmartSMSServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
